{
  "expo": {
    "name": "T-Track",
    "description": "Find out how much time you actually spent on your activities or projects.",
    "slug": "t-track",
    "privacy": "public",
    "sdkVersion": "26.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "rayvr.ttrack"
    },
    "android": {
      "package": "rayvr.ttrack"
    }
  }
}
